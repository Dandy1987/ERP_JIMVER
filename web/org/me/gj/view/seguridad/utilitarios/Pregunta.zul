    <hbox vflex="1" hflex="1" align="center" pack="center">
        <vlayout>
            <div id="div_pregunta" apply="org.me.gj.controller.seguridad.mantenimiento.ControllerLogin" >
                <grid id="grd_pregunta" sclass="grilla"  width="600px" height="auto">
