                        <row>
                            <cell width="10px">
                                <label value="RESPUESTA :"  />
                            </cell>
                            <textbox id="txt_rpta" focus="true" maxlength="50" width="450px" />
                        </row>
                        <row > 
                                <button id="btn_ok" label="Aceptar" sclass="botones"/>
