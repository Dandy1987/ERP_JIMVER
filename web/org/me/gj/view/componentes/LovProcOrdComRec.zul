                        </hbox>
                        <groupbox style="background-color:#FFFFFF" closable="false" width="985px">
                            <caption label="DATOS DEL DETALLE - ORDEN DE COMPRA" />
                            <vbox>
                                <hbox align="center">         
                                    <label value="ID: " />
                                    <intbox id="txt_ocd_comdet_key"  width="15px" maxlength="3" disabled="true" tooltiptext="Define el número de operación del detalle"  />                                                
                                    <space/>
                                    <label value="PRODUCTO: " />
                                    <span class="colorRojo">*</span>
                                    <textbox id="txt_ocd_prodid"  width="60px" maxlength="9" disabled="true" tooltiptext="Define el código del producto, máximo 9 caracteres"  />                                                
                                    <textbox id="txt_ocd_proddes" width="400px" maxlength="60" disabled="true" tooltiptext="Define la descripción del producto, máximo 60 caracteres"/>
                                    <space width="24px"/>
                                    <label value="PESO U.: " />
                                    <doublebox id="txt_ocd_pesounit" width="40px" maxlength="7" disabled="true" locale="en_US" format="##,##0.####" style="text-align: right;"  tooltiptext="Define el peso unitario"  />                                                                                            
                                    <textbox id="txt_ocd_peso_und" width="25px" maxlength="3" disabled="true" tooltiptext="Define la descripción del peso unitario" />                                            
                                    <space width="27px"/>
                                    <label value="VOL. U.: " />
                                    <doublebox id="txt_voluunit" width="40px" maxlength="7" disabled="true" locale="en_US" format="##,##0.####" style="text-align: right;"  tooltiptext="Define el volumen unitario"  />                                                                                            
                                    <textbox id="txt_vol_und" width="25px" maxlength="3" disabled="true" tooltiptext="Define la descripcion del volumen unitario" />                                            
                                </hbox>    
                                <hbox align="center">    
                                    <label value="CANT.: " />
                                    <span class="colorRojo">*</span>
                                    <doublebox id="txt_ocd_cantped" width="58px" maxlength="9" disabled="true" locale="en_US" format="##,##0.####" style="text-align: right;"  tooltiptext="Define la cantidad entera del producto"  />                                            
                                    <space/>
                                    <label value="P. UNIT.:" />
                                    <doublebox id="txt_ocd_precom" width="45px" maxlength="10" disabled="true" locale="en_US" format="##,##0.####" style="text-align: right;"  tooltiptext="Define el precio unitario del producto"  />                                            
                                    <space/>
                                    <label value="UND. VTA.:" />
                                    <textbox id="txt_odc_prounimanven" width="45px" maxlength="10" disabled="true" style="text-align: right;"  tooltiptext="Define la unidad de venta del producto"  />                                            
                                    <space/>                                                
                                    <label value="V. AFECTO:" />
                                    <doublebox id="txt_odc_valafe" width="75px" maxlength="10" disabled="true" locale="en_US" format="##,##0.####" style="text-align: right;"  tooltiptext="Define el valor afecto del producto"  />
